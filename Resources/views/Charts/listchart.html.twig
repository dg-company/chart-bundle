{% if compact == false %}

    <div class="box">
        <div class="box-header">
            <h3 class="box-title">{{ data.title }}</h3>
        </div>
        <div class="box-body">

{% endif %}

{% if data.values != null %}
    <table class="table table-bordered table-striped dataTable">
        <thead>
        <tr role="row">
            {% for key in data.keys %}
                <th>{{ key.label }}</th>
            {% endfor %}
        </tr>
        </thead>
        <tbody>
        {% for x,yValues in data.values %}
            <tr role="row">
                {% for key,keyInfo in data.keys %}
                    <td>
                        {{ yValues[key]|raw }}
                    </td>
                {% endfor %}
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endif %}

{% if compact == false %}

        </div>
    </div>
{% endif %}